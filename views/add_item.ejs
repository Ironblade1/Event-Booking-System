<% try { %>
    <%- include('partials/header', {user: [firstName, lastName]}) %>
  <%} catch(error) { %>
    <%- include('partials/header')%>
  <%} %>

<form style="padding: 5% 30%;" method="post" action="/add_item">
    <!-- Text input -->
    <div class="form-outline mb-4">
        <input name="addedTitle" type="text" id="form6Example4" class="form-control"
            placeholder="e.g., Car, Credit card bill " />
        <label class="form-label" for="form6Example4">Title</label>
    </div>
    <!-- Message input -->
    <div class="form-outline mb-4">
        <textarea class="form-control" id="form6Example7" rows="4"></textarea>
        <label name="addedDescription" class="form-label" for="form6Example7">Description</label>
    </div>
    <!-- Submit button -->
    <button type="submit" class="btn btn-primary btn-block mb-4">Submit</button>
</form>






<%- include('partials/footer') %>